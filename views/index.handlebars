<img src = "/images/burger-flat.png" class = "img-responsive center-block" alt="burger">
</img>
<h1 class = "text-center">Eat Da Burger!</h1>

<ol>
{{#each burger}}
	<li>{{this.burger_name}} </li>
{{/each}}
</ol>





<form action="/" method="POST">
  <div class="form-group">
    <label for="input">Your Burger</label>
    <input type="text" class="form-control" name="input" placeholder="your new Burger">
  </div>
  <button type="submit" class="btn btn-default" id="submit">Submit</button>
</form>

